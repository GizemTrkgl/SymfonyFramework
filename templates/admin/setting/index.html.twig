{% extends 'adminbase.html.twig' %}

{% block title %}Hello AdminController!{% endblock %}

{% block body %}
MAIN CONTENT
*********************************************************************************************************************************************************** -->
<!--main content start-->
<section id="main-content">
    <section class="wrapper site-min-height">

        <div class="row mt">
            <div class="col-lg-12">

    <h1>Setting Page</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Title</th>

                <th>Company</th>

                <th>Email</th>

                <th>Status</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for setting in settings %}
            <tr>
                <td>{{ setting.id }}</td>
                <td>{{ setting.title }}</td>
                <td>{{ setting.company }}</td>
                <td>{{ setting.email }}</td>
                <td>{{ setting.status }}</td>
                <td>
                    <a href="{{ path('setting_show', {'id': setting.id}) }}" class="btn btn-round btn-info">show</a>
                    <a href="{{ path('setting_edit', {'id': setting.id}) }}" class="btn btn-round btn-info">edit</a>

                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="21">no records found</td>
                <a href="{{ path('setting_new') }}" class="btn btn-round btn-info">Create Setting</a>
            </tr>
        {% endfor %}
        </tbody>
    </table>



            </div>
        </div>
    </section>
    <!-- /wrapper -->
</section>
    <!-- /MAIN CONTENT -->

{% endblock %}

